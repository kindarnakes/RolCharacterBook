<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.RolCharacterBook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ejemplo">
        <activity android:name=".view.Search"></activity>
        <activity
            android:name=".view.About"
            android:label="@string/title_activity_about"
            android:parentActivityName=".view.List"
            android:theme="@style/Theme.Ejemplo.NoActionBar" />
        <activity
            android:name=".view.List"
            android:label="@string/title_activity_list"
            android:theme="@style/Theme.Ejemplo.NoActionBar" />
        <activity
            android:name=".view.Form"
            android:label="@string/title_activity_Form"
            android:parentActivityName=".view.List"
            android:theme="@style/Theme.Ejemplo.NoActionBar" />
        <activity
            android:name=".view.SplashScreen"
            android:label="@string/app_name"
            android:theme="@style/Theme.Ejemplo.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>